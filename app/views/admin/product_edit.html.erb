<%= form_for @product, url: update_product_path(@product), method: :patch do |f| %>
	<div>
		<label> Name </label>
		<%= f.text_field :name %> <br> <br>
		<label> SKU ID </label>
		<%= f.text_field :sku_id %> <br> <br>
		<label> Categories </label>
		<%= f.text_field :categories %> <br> <br>
		<label> Tags </label>
		<%= f.text_field :tags %> <br> <br>
		<label> Price </label>
		<%= f.text_field :price %> <br> <br>
	</div>
	<%= f.submit "Submit" %>
<% end %>